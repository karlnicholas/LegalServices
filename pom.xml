<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>com.github.karlnicholas.legalservices</groupId>
    <artifactId>legalservices</artifactId>
    <version>2.0.6-SNAPSHOT</version>
    <packaging>pom</packaging>
    <name>Legal Services</name>
    <description>Legal Services</description>
    <properties>
        <spring.boot.version>2.5.2</spring.boot.version>
        <version.lucene>8.9.0</version.lucene>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <!-- maven-compiler-plugin -->
        <version.compiler.plugin>3.8.1</version.compiler.plugin>
        <maven.compiler.target>11</maven.compiler.target>
        <maven.compiler.source>11</maven.compiler.source>
        <maven.deploy.skip>true</maven.deploy.skip>
    </properties>
    <licenses>
        <license>
            <name>Apache License, Version 2.0</name>
            <distribution>repo</distribution>
            <url>http://www.apache.org/licenses/LICENSE-2.0.html</url>
        </license>
    </licenses>
    <developers>
        <developer>
            <name>Karl Nicholas</name>
            <email>karl.nicholas@outlook.com</email>
            <organizationUrl>https://github.com/karlnicholas</organizationUrl>
        </developer>
    </developers>
    <scm>
        <connection>https://github.com/karlnicholas/LegalServices.git</connection>
        <developerConnection>scm:git:git@github.com:karlnicholas/LegalServices.git</developerConnection>
        <url>https://github.com/karlnicholas/LegalServices</url>
        <tag>HEAD</tag>
    </scm>
    <modules>
        <module>Statute</module>
        <module>StatuteService</module>
        <module>StatuteServiceReactive</module>
        <module>CaseListModel</module>
        <module>SqlUtil</module>
        <module>OpinionService</module>
