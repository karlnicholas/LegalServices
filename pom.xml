                <artifactId>opinionservice</artifactId>
                <version>${project.version}</version>
                <type>jar</type>
                <scope>compile</scope>
            </dependency>
            <dependency>
                <groupId>com.github.karlnicholas.legalservices</groupId>
                <artifactId>opinionservicereactive</artifactId>
